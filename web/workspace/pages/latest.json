{
  "page": {
    "key": "Latest",
    "color": "#191502"
  },
  "settings": {
    "cache": true
  },
  "datasources": [
    "latest",
    "latest-footer",
    "github-api",
    "github-cdn",
    "github-web"
  ],
  "routes": [
    {
      "path": "/:lang/latest/"
    },
    {
      "path": "/:lang/latest/:category/:article?",
      "params": [
        {
          "param": "category",
          "in": ["articles", "roadmap", "releases"]
        }
      ]
    }
  ]
}
